{
  "name": "PulseWebApp",
  "version": "0.1.0",
  "description": "Central Office Pulse Web Application",
  "scripts": {
    "setup": "node tools/setupMessage.js && npm install",
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message remove-dist",
    "start": "npm-run-all --parallel test:watch open:src lint:watch",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint": "esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "build:html": "babel-node tools/buildHtml.js",
    "prebuild": "npm run clean-dist && npm run build:html && npm run lint && npm run test",
    "proxy": "forever tools/proxy-es5-wrapper.js",
    "build": "babel-node tools/build.js",
    "test": "mocha tools/testSetup.js tools/testDomSetup.js \"src/**/*.spec.js\" --reporter progress",
    "test:report": "rimraf ./mochawesome-reports && mocha tools/testSetup.js tools/testDomSetup.js \"src/**/*.spec.js\" --reporter mochawesome --reporter-options reportName=pulse-web-unit-test,reportTitle=pulse-web-specs,inlineAssets=true",
    "test:cover": "babel-node node_modules/isparta/bin/isparta cover --root src --report html node_modules/mocha/bin/_mocha -- --require ./tools/testSetup.js tools/testDomSetup.js \"src/**/*.spec.js\" --reporter progress",
    "test:watch": "npm run test -- --watch",
    "test:e2e": "node_modules/.bin/nightwatch --env"
  },
  "author": "PraveenRampally",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "6.9.1",
    "babel-preset-stage-2": "6.11.0",
    "bootstrap": "3.3.6",
    "chromedriver": "^2.24.1",
    "cucumber": "1.2.2",
    "iedriver": "^2.53.1",
    "isomorphic-fetch": "2.2.1",
    "geckodriver": "^1.1.3",
    "lodash": "4.16.1",
    "object-assign": "4.1.0",
    "react": "15.3.2",
    "react-dom": "15.1.0",
    "react-intl": "2.1.3",
    "react-redux": "4.4.5",
    "react-router": "2.6.0",
    "react-router-redux": "4.0.5",
    "redux": "3.5.2",
    "nightwatch": "0.9.8",
    "nightwatch-cucumber": "4.2.0",
    "selenium-server": "^3.0.1"
  },
  "devDependencies": {
    "babel-cli": "6.10.1",
    "babel-core": "6.10.4",
    "babel-loader": "6.2.4",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-1": "6.5.0",
    "babel-register": "6.9.0",
    "body-parser": "1.14.1",
    "browser-sync": "2.13.0",
    "chai": "3.5.0",
    "chalk": "1.1.3",
    "cheerio": "0.20.0",
    "connect-history-api-fallback": "1.2.0",
    "coveralls": "2.11.9",
    "cross-env": "1.0.8",
    "css-loader": "0.23.1",
    "enzyme": "2.3.0",
    "eslint": "2.13.1",
    "eslint-plugin-import": "1.9.2",
    "eslint-plugin-jsx-a11y": "1.5.3",
    "eslint-plugin-react": "5.2.2",
    "eslint-watch": "2.1.11",
    "express": "4.13.3",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "font-awesome": "4.6.3",
    "http": "0.0.0",
    "http-proxy": "1.12.0",
    "isparta": "4.0.0",
    "istanbul": "0.4.4",
    "jsdom": "9.5.0",
    "jsdom-global": "2.1.0",
    "mocha": "2.5.3",
    "mochawesome": "1.5.4",
    "node-sass": "3.8.0",
    "npm-run-all": "2.3.0",
    "prompt": "1.0.0",
    "react-addons-test-utils": "15.1.0",
    "react-intl-redux": "^0.1.1",
    "redux-immutable-state-invariant": "1.2.3",
    "replace": "0.3.0",
    "rimraf": "2.5.2",
    "sass-loader": "4.0.0",
    "sinon": "1.17.4",
    "sinon-chai": "2.8.0",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.1",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.11.0"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "http://almgit.ncr.com/projects/NR1/repos/nr1-pulsewebapp/browse"
  }
}
